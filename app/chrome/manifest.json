{
    "manifest_version": 2,
    "name": "JSON Editor",
    "version": "2.1.1",
    "description": "JSON Editor is a tool to view, edit, and format JSON. It shows your data in an editable treeview and in a code editor.",
    "app": {
        "urls": [
            "http://www.jsoneditoronline.cn/",
            "http://www.jsoneditoronline.cn/index.html",
